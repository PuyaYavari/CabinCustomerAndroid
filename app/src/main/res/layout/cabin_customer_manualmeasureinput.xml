    <ImageView
            android:layout_width="115dp"
            android:layout_height="320dp" app:srcCompat="@drawable/woman_figure"
            android:id="@+id/manualmeasureinput_womanfigure"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.1" app:layout_constraintVertical_bias="0.25"
            android:contentDescription="woman figure"/>
    <ImageView
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:id="@+id/manualmeasureinput_height_input_background" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/manualmeasureinput_womanfigure"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintVertical_bias="0.15" android:background="@drawable/cabin_grey_text_background"/>
    <TextView
            android:text="@string/persons_height"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/manualmeasureinput_height_text"
            app:layout_constraintTop_toTopOf="@+id/manualmeasureinput_height_input_background"
            app:layout_constraintBottom_toBottomOf="@+id/manualmeasureinput_height_input_background"
            app:layout_constraintEnd_toStartOf="@+id/manualmeasureinput_height_input"
            app:layout_constraintStart_toStartOf="@+id/manualmeasureinput_height_input_background"
            android:textColor="@color/white" android:textSize="20sp" app:fontFamily="@font/century_gothic"/>
    <EditText
            android:layout_width="150dp"
            android:layout_height="45dp"
            android:inputType="number"
            android:maxLength="3"
            android:ems="10"
            android:id="@+id/manualmeasureinput_height_input"
            app:layout_constraintEnd_toEndOf="@+id/manualmeasureinput_height_input_background"
            app:layout_constraintBottom_toBottomOf="@+id/manualmeasureinput_height_input_background"
            app:layout_constraintStart_toStartOf="@+id/manualmeasureinput_height_input_background"
            app:layout_constraintTop_toTopOf="@+id/manualmeasureinput_height_input_background"
            app:layout_constraintHorizontal_bias="0.985" android:background="@drawable/cabin_white_text_background"
            android:paddingLeft="10dp" android:paddingRight="30dp"/>
    <TextView
            android:text="@string/centimeters_abbreviation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/manualmeasureinput_height_input_suffix"
            app:layout_constraintBottom_toBottomOf="@+id/manualmeasureinput_height_input"
            app:layout_constraintTop_toTopOf="@+id/manualmeasureinput_height_input"
            app:layout_constraintEnd_toEndOf="@+id/manualmeasureinput_height_input"
            app:layout_constraintStart_toStartOf="@+id/manualmeasureinput_height_input" android:textSize="18sp"
            app:layout_constraintHorizontal_bias="0.98"/>
